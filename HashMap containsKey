package com.company;

import com.sun.jdi.Value;

import java.util.*;

public class Main
{
    public static void main(String[] args)
    {
        Scanner in =new Scanner(System.in);
        HashMap<Integer,String>map=new HashMap<Integer,String>();
        System.out.print("Enter the size of the map : ");
        int size=in.nextInt();
        System.out.println("Enter the Key and Value ");
        for(int i=0;i<size;i++)
        {
            map.put(in.nextInt(), in.nextLine());
        }
        System.out.println("The Key Value pair is "+map);
        System.out.print("Enter the key to check its presence: ");
        int keys= in.nextInt();
        System.out.println("Does the  Key "+keys+" present in the HashMap? "+map.containsKey(keys));


    }

}







